<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


            <View
                android:id="@+id/v_divider_top"
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:layout_gravity="bottom"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:layout_weight="1"
                >

                <ImageView
                    android:id="@+id/userIconview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="50px"
                    android:layout_marginLeft="36dp"
                    android:layout_marginBottom="40px"
                    app:layout_constraintStart_toStartOf="parent"
                    app:srcCompat="@drawable/avatar"
                    tools:layout_editor_absoluteY="27dp" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                <TextView
                    android:id="@+id/userTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="36dp"
                    android:layout_marginLeft="65px"
                    android:layout_gravity="center_vertical"
                    android:text="登录/注册"
                    android:textSize="21sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    />
</LinearLayout>
                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="21dp"
                    android:layout_height="32dp"
                    android:layout_marginLeft="35px"
                    app:srcCompat="@drawable/man"
                    android:layout_gravity="center_vertical"
                   />
            </LinearLayout>

            <View
                android:id="@+id/v_dividerbottom"
                android:layout_width="match_parent"
                android:layout_height="@dimen/divider_height"
                android:layout_gravity="bottom"
                android:background="?android:attr/listDivider" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:layout_weight="1"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            >
        <ImageButton
            android:id="@+id/imageView3"
            android:layout_height="38dp"
            android:layout_width="37dp"
            android:scaleType="fitXY"
            android:background="#e0000000"
            android:layout_weight="1"
            android:layout_marginTop="30px"
            android:layout_gravity="center_horizontal"
            android:clickable="true"
            android:src="@drawable/star"
            android:onClick="myAtt"/>
            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="25px"
                android:text="我的关注"
                android:textSize="11sp"
                app:layout_constraintEnd_toEndOf="parent"
                />

    </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            >
            <ImageButton
                android:id="@+id/imageView4"
                android:layout_width="38dp"
                android:layout_height="37dp"
                android:layout_weight="1"
                android:scaleType="fitXY"
                android:background="#e0000000"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30px"
                app:srcCompat="@drawable/star" />
            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="25px"
                android:text="我的收藏"
                android:textSize="11sp"
                app:layout_constraintEnd_toEndOf="parent"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            >
            <ImageButton
                android:id="@+id/imageView5"
                android:layout_width="38dp"
                android:layout_height="37dp"
                android:layout_weight="1"
                android:scaleType="fitXY"
                android:background="#e0000000"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30px"
                app:srcCompat="@drawable/star" />
            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="25px"
                android:text="我的发帖"
                android:textSize="11sp"
                app:layout_constraintEnd_toEndOf="parent"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            >
            <ImageButton
                android:id="@+id/imageView6"
                android:layout_width="38dp"
                android:layout_height="37dp"
                android:layout_weight="1"
                android:scaleType="fitXY"
                android:background="#e0000000"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30px"

                app:srcCompat="@drawable/star" />
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="25px"
                android:text="最近浏览"
                android:textSize="11sp"
                app:layout_constraintEnd_toEndOf="parent"
                />
        </LinearLayout>
        </LinearLayout>
    </LinearLayout>
        <View
            android:id="@+id/v_divider"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_gravity="bottom"
            android:background="?android:attr/listDivider" />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:layout_weight="4"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="35dp"
            android:layout_height="42dp"
            android:layout_marginLeft="70px"
            android:layout_gravity="center_vertical"
            app:srcCompat="@drawable/msg" />
        <TextView
            android:id="@+id/textView5"
            android:layout_width="120dp"
            android:layout_marginLeft="90px"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="系统通知"
            android:textSize="15sp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            />

    </LinearLayout>
        <View
            android:id="@+id/v_divider1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_gravity="bottom"
            android:background="?android:attr/listDivider" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"

            >
            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="35dp"
                android:layout_height="42dp"
                android:layout_marginLeft="70px"
                android:layout_gravity="center_vertical"
                app:srcCompat="@drawable/ask" />
            <TextView
                android:id="@+id/textView6"
                android:layout_width="120dp"
                android:layout_marginLeft="90px"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="帮助中心"
                android:textSize="15sp"
                android:clickable="true"
                app:layout_constraintEnd_toEndOf="parent"
                />

        </LinearLayout>
        <View
            android:id="@+id/v_divider2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_gravity="bottom"
            android:background="?android:attr/listDivider" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"

            >
            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="35dp"
                android:layout_height="42dp"
                android:layout_marginLeft="70px"
                android:layout_gravity="center_vertical"
                app:srcCompat="@drawable/book" />
            <TextView
                android:id="@+id/textView8"
                android:layout_width="120dp"
                android:layout_marginLeft="90px"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="意见反馈"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                />

        </LinearLayout>
        <View
            android:id="@+id/v_divider3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_gravity="bottom"
            android:background="?android:attr/listDivider" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"

            >
            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="35dp"
                android:layout_height="42dp"
                android:layout_marginLeft="70px"
                android:layout_gravity="center_vertical"
                app:srcCompat="@drawable/reflect" />
            <TextView
                android:id="@+id/textView9"
                android:layout_width="120dp"
                android:layout_marginLeft="90px"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="合作协议"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                />

        </LinearLayout>
        <View
            android:id="@+id/v_divider4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_gravity="bottom"
            android:background="?android:attr/listDivider" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"

            >
            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="35dp"
                android:layout_height="42dp"
                android:layout_marginLeft="70px"
                android:layout_gravity="center_vertical"
                app:srcCompat="@drawable/settings" />
            <TextView
                android:id="@+id/textView10"
                android:layout_width="120dp"
                android:layout_marginLeft="90px"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="设置"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                />

        </LinearLayout>
        <View
            android:id="@+id/v_divider5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/divider_height"
            android:layout_gravity="bottom"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>